{
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "/usr/include/",
        "/usr/include/**",
        "/usr/include/linux",
        "${workspaceFolder}",
        "${workspaceFolder}/src",
        "${workspaceFolder}/src/**/*",
        "${workspaceFolder}/sdk/include/**",
        "${workspaceFolder}/src/xrCore/",
        "${workspaceFolder}/**"
      ],
      "defines": [],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "intelliSenseMode": "linux-gcc-x64",
      "configurationProvider": "ms-vscode.cmake-tools",
      "browse": {
        "limitSymbolsToIncludedHeaders": false,
        "databaseFilename": "${workspaceFolder}/.vscode/vs.db"
      }
    }
  ],
  "version": 4
}
